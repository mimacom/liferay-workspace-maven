Bundle-Name: mimacom Training REST (Swagger)
Bundle-SymbolicName: com.mimacom.training.rest
Bundle-Version: 1.0.0-SNAPSHOT

Import-Package: !javax.validation.*,\
                !javax.activation,\
                *

-includeresource: META-INF/lib/swagger-annotations.jar=swagger-annotations-2.0.6.jar;lib:=true,\
                  META-INF/lib/jackson-annotations.jar=jackson-annotations-[0-9]*.jar;lib:=true,\
