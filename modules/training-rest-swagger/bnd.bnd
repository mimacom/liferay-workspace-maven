                  META-INF/lib/jackson-databind.jar=jackson-databind-[0-9]*.jar;lib:=true,\
                  META-INF/lib/jackson-jaxrs-base.jar=jackson-jaxrs-base-[0-9]*.jar;lib:=true,\
                  META-INF/lib/jackson-jaxrs-json-provider.jar=jackson-jaxrs-json-provider-[0-9]*.jar;lib:=true,\
                  META-INF/lib/jackson-module-jaxb-annotations.jar=jackson-module-jaxb-annotations-[0-9]*.jar;lib:=true